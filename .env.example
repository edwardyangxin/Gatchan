# Telegram bot token from @BotFather
TELEGRAM_BOT_TOKEN=

# Telegram webhook secret token
TELEGRAM_WEBHOOK_SECRET=

# Optional whitelist (comma-separated integers). Leave empty to allow all.
TELEGRAM_ALLOWED_USER_IDS=
TELEGRAM_ALLOWED_CHAT_IDS=
TELEGRAM_WHITELIST_REPLY=false

# Todoist personal API token
TODOIST_API_TOKEN=

# Main task name for "todo later" (auto-created if missing)
TODO_LATER_TASK_NAME=todo later

# Optional transcription provider (openai or gemini)
TRANSCRIBE_PROVIDER=

# Optional provider API keys
OPENAI_API_KEY=
GEMINI_API_KEY=
